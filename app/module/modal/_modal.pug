mixin modal()
	.modal-layer
		.modal-filter
		.modal-wrap
			// phone enter
			.modal(data-modal="enter")
				.modal-content
					.modal-close
						+icon("close","icon")
					.modal-title Вход на сайт
					.input-row
						input(type="phone" name="phone" placeholder="Телефон" ).input.input-mask--phone
					.input-row
						input(type="password" name="password" placeholder="Пароль").input
					.btn--md.btn--mark.btn--full Войти
					.modal-link-wrap
						a(href="#").modal-link Забыли пароль
						a(href="#").modal-link Регистрация
			// phone enter === end

			// enterPhone
			.modal(data-modal="enterPhone")
				.modal-content
					.modal-close
						+icon("close","icon")
					.modal-title Восстановление	пароля
					.modal-text Введите номер телефона и мы отправим на него код для восстановления пароля
					.input-cont
						.input-row
							input(type="phone" name="phone" placeholder="Телефон").input.input-mask--phone
					.modal-send.mb-24 Отправить
					.flex.flex--mid
						a(href="#").modal-link Я вспомнил пароль
			// enterPhone === end

			// enterCode
			.modal(data-modal="enterCode")
				.modal-content
					.modal-close
						+icon("close","icon")
					.modal-title Восстановление	пароля
					.modal-text Введите код, который мы выслали на ваш номер телефона по СМС
					.input-cont
						.input-row
							input(type="text" name="text" placeholder="Код").input
					.btn--md.btn--mark.btn--full.mb-24 Отправить
					.flex.flex--between.js-modal-countdown-wrap(data-time="12")
						.link--mark.type--dark.type--disable.js-modal-countdown-type Отправить код еще раз
						.countdown-cont
							+icon("clock","icon icon--sm")
							.type--sm
								span.js-modal-countdown-val 60
								| &nbsp;сек
			// forgetCode === end

			// enterPass
			.modal(data-modal="enterPass")
				.modal-content
					.modal-close
						+icon("close","icon")
					.modal-title Восстановление	пароля
					.modal-text Введите новый пароль для входа
					.input-cont
						.input-row
							input(type="password" name="password" placeholder="Новый пароль").input
					.input-cont
						.input-row
							input(type="password" name="password" placeholder="Повторите пароль").input
					.modal-send Сохранить
			// enterPass === end

			// enterPassTrue
			.modal(data-modal="enterPassTrue")
				.modal-content
					.modal-close
						+icon("close","icon")
					.modal-title Пароль изменен успешно!
					.modal-text Теперь вы можете войти в личный кабинет, сохранить свои адреса, копить или списывать бонусы и следить за историей заказов.
					.btn--md.btn--mark.btn--full Войти
			// enterPassTrue === end

			// reg
			.modal(data-modal="reg")
				.modal-content
					.modal-close
						+icon("close","icon")
					.modal-title Регистрация
					.input-cont
						.input-row
							input(type="phone" name="phone" placeholder="Телефон").input.input-mask--phone
					.login-remember
						.check-el
							label.style-label.style-label--checkbox
								input(type="checkbox").style-input
								.style-input-element
									+icon("checked","icon")
								.style-input-text Я ознакомлен с #[a(href="#") пользовательским соглашением] &nbsp; и даю согласие на обработку моих #[a(href="#") персональных данных]
					.btn--md.btn--mark.btn--full.mb-24.modal-get(data-modal="") Далее
					.flex.flex--mid
						a(href="#").modal-link У меня есть аккаунт
			// reg === end

			// regCode
			.modal(data-modal="regCode")
				.modal-content
					.modal-close
						+icon("close","icon")
					.modal-title Регистрация
					.modal-text Введите код, который мы выслали на ваш номер телефона по СМС
					.input-cont
						.input-row
							input(type="text" name="text" placeholder="Код").input
					.btn--md.btn--mark.btn--full.mb-24 Отправить
					.flex.flex--between.js-modal-countdown-wrap(data-time="10")
						.type--gray.type--disable.link--mark.js-modal-countdown-type Отправить код еще раз
						.icon-row.countdown-cont
							+icon("clock","icon icon--sm")
							.type--sm
								span.js-modal-countdown-val 60
								| &nbsp;сек
			// regCode === end

			// regData
			.modal(data-modal="regData")
				.modal-content
					.modal-close
						+icon("close","icon")
					.modal-title Регистрация
					.input-cont
						.input-row
							input(type="text" name="name" placeholder="Ваше имя").input
					.input-cont
						.input-row
							input(type="password" name="password" placeholder="Придумайте пароль").input
					.input-cont
						.input-row
							input(type="password" name="password" placeholder="Повторите пароль").input
					.btn--md.btn--mark.btn--full.modal-get(data-modal="") Далее
			// regData === end

			// regMail
			.modal(data-modal="regMail")
				.modal-content
					.modal-close
						+icon("close","icon")
