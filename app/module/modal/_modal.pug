					.input-cont
						.input-row
							input(type="phone" name="phone" placeholder="Телефон" class="input").input-mask--phone
					.input-row
						textarea(name="info", cols="30", rows="6" placeholder="Сообщение").input
					+deal()
					.btn--md.btn--mark.btn--full Отправить
			// review === end

			// reviewTrue action
			.modal(data-modal="reviewTrue")
